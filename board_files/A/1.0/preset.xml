<?xml version="1.0" encoding="UTF-8"?>
<preset xmlns="http://www.xilinx.com/XMLSchema/preset">
  <ps7_preset>
    <version>1.0</version>
    <ddr>
      <enable>true</enable>
      <memory_type>DDR3</memory_type>
      <memory_size>512MB</memory_size>
    </ddr>
    <qspi>
      <enable>true</enable>
      <io>single</io>
    </qspi>
    <ethernet>
      <enable>true</enable>
      <interface>rgmii</interface>
    </ethernet>
    <usb>
      <enable>true</enable>
      <interface>ulpi</interface>
    </usb>
    <uart>
      <enable>true</enable>
      <number>0</number>
    </uart>
  </ps7_preset>
</preset>
